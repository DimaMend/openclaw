{
  "name": "@moltbot/ringcentral",
  "version": "2026.1.28",
  "type": "module",
  "description": "Moltbot RingCentral Team Messaging channel plugin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/moltbot/moltbot.git",
    "directory": "extensions/ringcentral"
  },
  "keywords": [
    "moltbot",
    "ringcentral",
    "team-messaging",
    "chat",
    "bot"
  ],
  "files": [
    "src",
    "index.ts",
    "README.md"
  ],
  "moltbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "ringcentral",
