    "install": {
      "npmSpec": "@moltbot/ringcentral",
      "localPath": "extensions/ringcentral",
      "defaultChoice": "npm"
    }
  },
  "dependencies": {
    "@ringcentral/sdk": "^5.0.0",
    "@ringcentral/subscriptions": "^5.0.0",
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "moltbot": "workspace:*"
  },
  "peerDependencies": {
    "moltbot": ">=2026.1.25"
  }
